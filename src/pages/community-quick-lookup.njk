---
title: CRS Community Quick Lookup
permalink: "/cql/"
---


<table id="example" class="display" style="width:100%">
  <thead>
    <tr>
      <th>CID</th>
      <th>Community Name</th>
      <th>State</th>
      <th>Class</th>
    </tr>
  </thead>
  <tbody>
  {% for community in communities %}
    <tr>
      <td>2000{{ 10 + loop.index }}</td>
      <td>{{ community.name + [', City of', ', Town of'] | random }}</td>
      <td>{{ community.state }}</td>
      <td>{{ [4,5,6,7,8,9] | random }}</td>
    </tr>
  {% endfor %}
  </tbody>
  <tfoot>
    <tr>
      <th>CID</th>
      <th>Community Name</th>
      <th>State</th>
      <th>Class</th>
    </tr>
  </tfoot>
</table>
